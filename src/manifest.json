{
  "name": "bx24dev",
  "description": "Extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "bx24dev"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "<all_urls>",
    "webNavigation",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://localhost:*; object-src 'self';"
}
