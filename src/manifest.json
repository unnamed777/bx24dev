{
  "name": "bx24dev",
  "description": "Bitrix24 Dev extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "bx24dev"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "<all_urls>",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "contextualIdentities"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:*; object-src 'self';",
  "web_accessible_resources": ["tab/index.html", "popup/popup.html", "webhook/index.html"]
}
